<?xml version="1.0" encoding="utf-8" ?>
<odoo>
     <record model="ir.ui.view" id="view_stock_move_line_detailed_operation_tree">
         <field name="model">stock.move.line</field>
         <field
      name="inherit_id"
      ref="sale_order_line_containers.view_stock_move_line_detailed_operation_tree"
    />
         <field name="arch" type="xml">
            <field name="container" position="before">
                <field
          name="product_packaging"
          optional="show"
          domain="[('product_id', '=', product_id)]"
        />
                <field name="package_qty" optional="show" />
            </field>
         </field>
     </record>
</odoo>
