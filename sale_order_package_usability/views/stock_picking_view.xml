<?xml version="1.0" encoding="utf-8" ?>
<odoo>
     <record model="ir.ui.view" id="view_picking_form">
         <field name="model">stock.picking</field>
         <field name="inherit_id" ref="sale_order_line_containers.view_picking_form" />
         <field name="arch" type="xml">
            <xpath
        expr="//field[@name='move_ids_without_package']/tree/field[@name='container']"
        position="before"
      >
                <field name="product_packaging" optional="show" />
                <field name="package_qty" optional="show" />
            </xpath>
         </field>
     </record>
</odoo>
