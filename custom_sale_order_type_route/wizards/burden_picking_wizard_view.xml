<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="burden_picking_wizard_view_form" model="ir.ui.view">
        <field name="name">burden.picking.wizard.form</field>
        <field name="model">burden.picking.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="text" readonly="1" nolabel="1" />
                    </group>
                    <footer>
                        <button
              name="button_create_burden_picking"
              string="Accept"
              type="object"
              class="oe_highlight"
            />
                        or
                        <button special="cancel" string="Cancel" class="oe_link" />
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
